{
    "name": "File Sync BETA",
    "type": "plugin",
    "icon": "gen-loop-2",
    "description": {
        "short": "Sync shared folders across devices with Pulse",
        "long": "Pulse replaces Dropbox and BitTorrent Sync with something open, trustworthy and decentralized. Your data is your data alone and you deserve to choose where it is stored, if it is shared with some third party and how it's transmitted over the Internet."
    },
    "categories": [
        {
            "primary": "File Storage",
            "secondary": ["Documents", "Music", "Photos"]
        }
    ],
    "version": "0.5",
    "author": "arkOS",
    "homepage": "https://arkos.io",
    "app_author": "Ind.ie",
    "app_homepage": "https://ind.ie/pulse/",
    "logo": true,
    "screenshots": [],
    "services": [
        {
            "name": "File Sync",
            "binary": "syncthing@syncthing",
            "ports": [["tcp", "22000"], ["udp", "21025"]]
        }
    ],
    "modules": ["main", "backend"],
    "platforms": ["any"],
    "dependencies": {
        "any": [
            {
                "type": "app",
                "name": "Pulse",
                "package": "syncthing",
                "binary": "syncthing"
            }
        ]
    },
    "generation": 1
}
